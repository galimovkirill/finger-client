{
    "name": "@galimovdev/fg-ui",
    "private": false,
    "version": "0.0.8",
    "type": "module",
    "author": {
        "name": "Kirill Galimov",
        "email": "galimovdev@gmail.com"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/finger-ui.umd.cjs",
    "module": "./dist/finger-ui.js",
    "publishConfig": {
        "access": "public"
    },
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "types": "./dist/main.d.ts",
            "import": "./dist/finger-ui.js",
            "require": "./dist/finger-ui.umd.cjs"
        },
        "./css": {
            "import": "./dist/main.css",
            "require": "./dist/main.css"
        }
    },
    "scripts": {
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "init-msw": "msw init public/"
    },
    "dependencies": {
        "@vueuse/core": "^10.2.0",
        "lodash": "^4.17.21",
        "sass": "^1.63.4",
        "sass-loader": "^13.3.2",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.2",
        "@storybook/addon-a11y": "^7.0.22",
        "@storybook/addon-essentials": "^7.0.22",
        "@storybook/addon-interactions": "^7.0.22",
        "@storybook/addon-links": "^7.0.22",
        "@storybook/blocks": "^7.0.22",
        "@storybook/testing-library": "^0.1.0",
        "@storybook/vue3": "^7.0.22",
        "@storybook/vue3-vite": "^7.0.22",
        "@tsconfig/node18": "^2.0.1",
        "@types/node": "^18.16.8",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-prettier": "^7.1.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/tsconfig": "^0.4.0",
        "eslint": "^8.43.0",
        "eslint-plugin-vue": "^9.15.0",
        "msw": "^1.2.2",
        "msw-storybook-addon": "^1.8.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "rollup-plugin-typescript2": "^0.35.0",
        "storybook": "^7.0.22",
        "typescript": "~5.1.3",
        "vite": "^4.3.9",
        "vite-plugin-dts": "^3.0.0-beta.3",
        "vue-router": "^4.2.2",
        "vue-tsc": "^1.8.0"
    }
}
