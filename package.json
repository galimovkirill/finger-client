{
    "name": "@galimovdev/fg-ui",
    "version": "0.0.12",
    "type": "module",
    "main": "./dist/finger-ui.umd.ts",
    "module": "./dist/finger-ui.es.ts",
    "types": "./dist/components/index.d.ts",
    "author": {
        "name": "Kirill Galimov",
        "email": "galimovdev@gmail.com"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/finger-ui.es.ts",
            "require": "./dist/finger-ui.umd.ts"
        },
        "./css": {
            "import": "./dist/main.css",
            "require": "./dist/main.css"
        }
    },
    "scripts": {
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.dist.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "storybook": "storybook dev -p 6006 -p ./tsconfig.dev.json",
        "build-storybook": "storybook build",
        "prepare": "npm run build"
    },
    "dependencies": {
        "@vueuse/core": "^10.3.0",
        "lodash": "^4.17.21",
        "sass": "^1.66.1",
        "sass-loader": "^13.3.2",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@storybook/addon-a11y": "^7.3.2",
        "@storybook/addon-essentials": "^7.3.2",
        "@storybook/addon-interactions": "^7.3.2",
        "@storybook/addon-links": "^7.3.2",
        "@storybook/blocks": "^7.3.2",
        "@storybook/testing-library": "^0.2.0",
        "@storybook/vue3": "^7.3.2",
        "@storybook/vue3-vite": "^7.3.2",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "^18.16.8",
        "@vitejs/plugin-vue": "^4.3.1",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/tsconfig": "^0.4.0",
        "eslint": "^8.47.0",
        "eslint-plugin-vue": "^9.17.0",
        "msw": "^1.2.3",
        "msw-storybook-addon": "^1.8.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.2",
        "storybook": "^7.3.2",
        "typescript": "~5.1.6",
        "vite": "^4.4.9",
        "vite-plugin-dts": "^3.5.2",
        "vue-router": "^4.2.4",
        "vue-tsc": "^1.8.8"
    }
