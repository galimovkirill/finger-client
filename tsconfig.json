{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ES2021.String"
    ],
    "module": "ESNext",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "vue": [
        "./node_modules/@vue"
      ]
    },
    "types": [],
    "jsx": "preserve",
    "importsNotUsedAsValues": "remove",
    "exactOptionalPropertyTypes": false,
    "noPropertyAccessFromIndexSignature": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}